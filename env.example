# Configuración de Seguridad
# PIN para eliminar armazones (cambia este valor por uno seguro)
VITE_ADMIN_DELETE_PIN=tu_pin_seguro_aqui

# Ejemplo de PIN seguro: 
# VITE_ADMIN_DELETE_PIN=prueba123

# Optimizaciones de Auth (banderas opcionales)
# Cargar perfil solo en eventos clave de Auth (SIGNED_IN, USER_UPDATED, y carga inicial)
VITE_AUTH_EVENTS_MINIMAL=true

# Duración del caché de usuario (en milisegundos). Default 900000 (15 minutos)
VITE_AUTH_CACHE_DURATION_MS=900000
